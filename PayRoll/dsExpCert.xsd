          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="TripChargesTableAdapter" GeneratorDataComponentClassName="TripChargesTableAdapter" Name="TripCharges" UserDataComponentName="TripChargesTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="VanitaPayrollConnectionString (Web.config)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (100) PERCENT M_LocationDep.LocationDep, M_Status.Status, M_Emp.Employeecd, M_Emp.Employeename, M_Emp.OrigJoindate,
                             (SELECT        years
                               FROM            dbo.udfTimeSpan(M_Emp.OrigJoindate, @docDate) AS udfTimeSpan_3) AS TsYear,
                             (SELECT        months
                               FROM            dbo.udfTimeSpan(M_Emp.OrigJoindate, @docDate) AS udfTimeSpan_2) AS Tsmonths,
                             (SELECT        days
                               FROM            dbo.udfTimeSpan(M_Emp.OrigJoindate, @docDate) AS udfTimeSpan_1) AS Tsdays, M_Organization.Organization, CASE WHEN
                             (SELECT        years
                               FROM            dbo.udfTimeSpan(dbo.M_Emp.OrigJoindate, @docDate) AS udfTimeSpan_3) &gt;= 1 THEN 'Above 1 Year' ELSE 'Below 1 Year' END AS ServiceStatus, M_DI.Description AS DI
FROM            dbo.udfEmpConfigurationmax1(@OrgID, @DocDate, 'sta') AS sta RIGHT OUTER JOIN
                         M_Emp LEFT OUTER JOIN
                         M_Organization ON M_Emp.OrgId = M_Organization.OrgId LEFT OUTER JOIN
                         dbo.udfEmpConfigurationmax1(@OrgID, @DocDate, 'dept') AS dept ON M_Emp.OrgId = dept.OrgId AND M_Emp.Employeecd = dept.Employeecd ON sta.OrgId = M_Emp.OrgId AND 
                         sta.Employeecd = M_Emp.Employeecd LEFT OUTER JOIN
                         M_Status ON sta.conId = M_Status.Stacd LEFT OUTER JOIN
                         M_LocationDep ON dept.conId = M_LocationDep.LocDepCd LEFT OUTER JOIN
                         dbo.udfEmpConfigurationmax1(@OrgID, @DocDate, 'div') AS div ON M_Emp.OrgId = div.OrgId AND M_Emp.Employeecd = div.Employeecd LEFT OUTER JOIN
                         dbo.udfEmpConfigurationmax1(@OrgID, @DocDate, 'desg') AS desg ON M_Emp.OrgId = desg.OrgId AND M_Emp.Employeecd = desg.Employeecd LEFT OUTER JOIN
                         M_DI ON M_Emp.DiIncode = M_DI.DiIncode
WHERE        (M_Emp.OrgId = @OrgID) AND (M_Emp.Leavedate IS NULL OR
                         M_Emp.Leavedate &gt; @frtDate) AND (div.conId &lt;&gt; 5) OR
                         (M_Emp.OrgId = @OrgID) AND (M_Emp.Leavedate IS NULL OR
                         M_Emp.Leavedate &gt; @frtDate) AND (desg.conId IN (5, 6, 7, 9, 34))
ORDER BY M_Emp.Employeecd</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="OrgID" ColumnName="" DataSourceName="" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@OrgID" Precision="0" Scale="0" Size="0" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
